{
    "name": "crumbls/laravel-locks",
    "description": "Allow locking and unlocking models when a user is editing, etc",
    "license": "MIT",
    "authors": [
        {
            "name": "Chase C. Miller",
            "email": "chase@crumbls.com",
            "homepage": "https://github.com/chasecmiller"
        }
    ],
    "homepage": "Allow locking and unlocking of models when a user is editing. Similar to WordPress' Post locks.",
    "keywords": [
        "Laravel", "Lock", "Unlock", "Model", "Editing"
    ],
    "require": {
        "laravel/framework": "^8.0"
    },
    "require-dev": {
        "laravel/framework": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Crumbls\\Lock\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Crumbls\\Lock\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Crumbls\\Lock\\LockServiceProvider"
            ],
            "aliases": {
                "Lock": "Crumbls\\Lock\\Facades\\Lock"
            }
        }
    }
}